<h1>Listing events</h1>

<table>
  <tr>
    <th>Member</th>
    <th>Location</th>
    <th>Name</th>
    <th>Starts at</th>
    <th>Ends at</th>
    <th>Agenda</th>
    <th>Created at</th>
    <th>Minutes</th>
    <th>Status</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%=h event.member_id %></td>
    <td><%=h event.location_id %></td>
    <td><%=h event.name %></td>
    <td><%=h event.starts_at %></td>
    <td><%=h event.ends_at %></td>
    <td><%=h event.agenda %></td>
    <td><%=h event.created_at %></td>
    <td><%=h event.minutes %></td>
    <td><%=h event.status %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event', new_event_path %>